{
  "name": "backend-server-ley-rutas",
  "version": "1.0.0",
  "description": "Backen de Rutas para Volarys",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "start": "npm run build && node dist/index.js",
    "startw": "npm run build && npm run copyw && copy vapid.json dist  && node dist/index.js",
    "startl": "npm run build && npm run copyl && cp vapid.json dist && node dist/index.js",
    "copyw": "@powershell copy 'src/assets/*' 'dist/assets/'",
    "copyl": "cp -r src/assets/* dist/assets/",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w",
    "vapid": "web-push generate-vapid-keys --json > vapid.json && cp vapid.json dist  "
  },
  "engines": {
    "node": "10.16.2",
    "npm": "2.15.12"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/urlsafe-base64": "^1.0.28",
    "@types/web-push": "^3.3.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.10",
    "g": "^2.0.1",
    "google-auth-library": "^6.0.6",
    "googleapis": "^58.0.0",
    "jsonwebtoken": "^8.5.1",
    "latest": "^0.2.0",
    "mongoose": "^5.9.29",
    "mongoose-unique-validator": "^2.0.3",
    "nodemailer": "^6.4.11",
    "nodemon": "^2.0.4",
    "underscore": "^1.10.2",
    "uniqid": "^5.2.0",
    "urlsafe-base64": "^1.0.0",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/express-fileupload": "^0.4.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongoose": "^5.7.36",
    "@types/mongoose-unique-validator": "^1.0.4",
    "@types/nodemailer": "^6.4.0",
    "@types/underscore": "^1.10.21",
    "@types/uniqid": "^4.1.3",
    "rimraf": "^3.0.2"
  },
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true
  },
  "lib": [
    "es2015"
  ],
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
